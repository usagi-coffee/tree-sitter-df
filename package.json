{
  "name": "@usagi-coffee/tree-sitter-df",
  "version": "0.0.5",
  "description": "OpenEdge DF grammar for tree-sitter",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usagi-coffee/tree-sitter-df.git"
  },
  "funding": "",
  "license": "MIT",
  "author": {
    "name": "Kamil Jakubus",
    "email": "kamil.jakubus@usagi.coffee"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": ["incremental", "parsing", "tree-sitter", "df"],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter": "^0.22.4",
    "tree-sitter-cli": "^0.26.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "bun run tools/generate.js && bun run tools/test.js",
    "build": "bun run tools/generate.js && tree-sitter build",
    "build:wasm": "tree-sitter generate && tree-sitter build -w",
    "parse": "bun run tools/generate.js && tree-sitter parse",
    "parse:snippet": "bun run tools/generate.js && bun run tools/parse.js"
  }
}
